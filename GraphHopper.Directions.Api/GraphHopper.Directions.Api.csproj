<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>GraphHopper Directions API Client</Title>
    <Authors>Hrvoje Matic</Authors>
    <Company>Ibicom Ltd</Company>
    <Description>GraphHopper Directions API Client lightweight implementation of .NET client for optimizing delivery using GraphHopper direction API.</Description>
    <Copyright>Ibicom Ltd</Copyright>
    <PackageProjectUrl>https://github.com/HEBOS/graphhopper</PackageProjectUrl>
    <PackageTags>graphhopper;delivery;traveling;salesman;vehicle;routing;problem</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MoqRestSharp.Helpers" Version="2021.4.30" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="RestSharp" Version="108.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
